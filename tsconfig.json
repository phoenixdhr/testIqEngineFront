{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*" : ["./src/components/*"],
      "@graphql-astro/*" : ["src/graphql-astro/*"],
      "@layouts/*" : ["./src/layouts/*"],
      "@pages/*" : ["./src/pages/*"],
      "@styles/*" : ["./src/styles/*"],


    }
  }
}